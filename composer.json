{
    "name": "danielkulbe/googleanalytics",
    "description": "Analyse your Bolt site with Google Analytics.",
    "type": "bolt-extension",
    "keywords": ["google", "analytics", "widget"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        },
        {
            "name": "Daniel Kulbe",
            "email": "info@daniel-kulbe.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.daniel-kulbe.de/root/bolt-google-analytics.git"
        }
    ],
    "require": {
        "bolt/bolt": ">=2.2.0,<3.0.0",
        "php": ">=5.3"
    },
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\Bolt\\GoogleAnalytics\\": ""
        }
    },
    "extra": {
        "bolt-icon": "logo-google_analytics.png",
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}